{
    "name": "Weatherman",
    "version": "1.0",
    "manifest_version": 2,
    
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "WeatherMan",
        "default_icon": "rain.png"

    },
    "permissions": [
        "geolocation",
        "storage"

    ],
    "icons": {
        "16": "rain.png",
        "48": "rain.png",
       "128": "rain.png" }
}